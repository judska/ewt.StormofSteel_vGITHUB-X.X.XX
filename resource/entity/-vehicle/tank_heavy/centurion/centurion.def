{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{collider "tank.medium"}
	{props "medium" "turret_rotator" "vision_vehicle_maximum" "muzzle_large"}
	
	{extender "enumerator"}

	{Volume "body1"
		{tags "body"}
		{component "body"}
	}
	{Volume "body2"
		{tags "body"}
		{component "body"}
	}
	{Volume "body3"
		{tags "body"}
		{component "body"}
	}

	("armor_medium" turret(127 76 76 30) body(76 51 38 17))
	("armor_engine"  t(51 51 51 51))
	("armor_mantlet" t(127))
	("shield" args 10)

	{extender "shield"
		{on}
	}

	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldl3" {tags "shield"}}
	{component "shieldR1" {tags "shield"}}
	{component "shieldR2" {tags "shield"}}
	{component "shieldR3" {tags "shield"}}

	{extender "enumerator"}

	("crew_4_human")

	("crew_human_gunner" place(gunner2) visorbone(mgun_rot))
	{boarder
		{anm "left"
			{forward	{begin	"board_tank_right"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_left"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_lowArmor"} 	{end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_lowArmor"} {end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_body"}}
		}
		{anm "armor2"
			{forward	{begin	"board_lowArmor"} {end "pose_seat_armor_1"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit5)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit6)  animation(armor1))


	{extender "inventory"
		{box
      		{size 8 8}
			{item "ammo bullet76 ap" 43}
			{item "ammo bullet76 fg" 27}
			{item "ammo bullet76 apcr" 5}
			{item "browning" filled {user "mgun"}}
			{item "bullet20 zenite" 900}
			{item "ammo hmgun" 800}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "76mm_mk4" filling "ammo bullet76 ap" 1}
			("abm_high")
		}
		{place "gun2"
			{type "mgun_coaxial"}
			{weapon "20mm_oerlikon" filled}
			{gunner "gunner"}
			{charger "charger"}
			{foresight "foresight11"}
			("abm_zenite")
			{basic}
		}
		{place "mgun"
			{type "mgun_aa"}
			{gunner "gunner2"}
			{charger "gunner2"}
			{LinkBone "mgun_rot"}
			{LinkAnimation "off"}
			("abm_mgun_heavy")
			{basic}
		}
	}

	{Extension "centurions.mdl"}

	{mass 34200}
	{Chassis
		("tank_trace" fx("tracks_mid") step(0.50) len(0.63))
		{Locomotion ("locomotion")
			{MaxSpeed	28}
			{maxSpeedAtMaxTurn 11}
			{StartTime	17}
			{TurnTime	11}
			{TurnStart  1.8} 
    		{BrakeTime  1.5}
     		{StopTime   2.5} 
    		{TurnRadius 11} 
    		{TurnStop   0.8} 
    		{Gears      0.1 0.5 1.0}
    		{RearGears  0.1 0.28}
		}
		{FuelBag
			{volume 750 }	
			{rate	170 }   
			{remain 750 }
			{fuel "fuel"}
		}
		{speed
        	{Normal  10}
        	{Slow  12}
    	}
}

	{bone "turret"
		{speed 0.0071}
	}
	{bone "gun_rot"
		{limits -15 7}
		{speed 0.003}
	}
	{bone "turret_mgun"
		{limits -30 30}
		{speed 0.03}
		{terminator}
	}
}
