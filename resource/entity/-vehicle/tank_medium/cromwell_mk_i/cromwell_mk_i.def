{game_entity
	(include "/properties/tank.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.2))
	{props "medium" "turret_rotator" "vision_vehicle_high" "muzzle_medium"}
	
	{extender "enumerator"}

	{Volume "body1"
		{tags "body"}
		{component "body"}
	}
	{Volume "body2"
		{tags "body"}
		{component "body"}
	}
	{Volume "body3"
		{tags "body"}
		{component "body"}
	}
	{Volume "gun"
		{tags "gun"}
		{component "gun"}
	}
	{volume "engine"
		{tags "engine"}
		{component "engine"}
	}

	("armor_medium" turret(76 64 64 20) body(64 32 32 14) body2(25 32 32 14) body3(57 32 32 14))
	("armor_engine"  t(10 32 25 14))

	("shield" args 10)
	
	{extender "shield"
		{on}
	}
	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldl3" {tags "shield"}}
	{component "shieldl4" {tags "shield"}}
	{component "shieldr1" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}
	{component "shieldr3" {tags "shield"}}
	{component "shieldr4" {tags "shield"}}
	("crew_4_human")


	{boarder
		{anm "left"
			{forward	{begin	"board_tank_right"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_left"} {end "pose_mgun_pz6"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_lowArmor"} 	{end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_lowArmor"} {end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_body"}}
		}
		{anm "armor2"
			{forward	{begin	"board_lowArmor"} {end "pose_seat_armor_1"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("crew_human_visible" 	place(gunner2)	door1(emit1) door2(emit2) visor(main))



	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit4)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit5)  animation(armor))
	("seat_human" place(seat4) bone(seat04) door(emit6)  animation(armor1))


	{extender "inventory"
		{box
            {item "ammo bullet57 ap" 34}
            {item "ammo bullet57 fg" 29}
            {item "ammo bullet57 apcr" 5}
            {item "ammo mgun" 2475}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "57mm_oqf" filling "ammo bullet57 ap" 1}
			("abm_high")
		}

		{place "mgun1"
			{type "mgun_hull"}
			{weapon "besa" filled}
			{foresight "foresight4"}
			{gunner "driver"}
			{charger "driver"}
			("abm_mgun")
		}
		{place "mgun2"
			{type "mgun_coaxial"}
			{weapon "besa" filled}
			{foresight "foresight5"}
			("abm_mgun")
			{basic}
		}
	}

	{mass 28000}
	{Chassis
		("tank_trace" fx("tracks_mid") step(0.52) len(0.2))
		{Locomotion ("locomotion")
            {MaxSpeed   36}
			{maxSpeedAtMaxTurn 14}
			{StartTime  15}
			{TurnStart  1.7}			
			{TurnTime   8}
			{TurnRadius 9} 
    		{TurnStop   0.8}
			{BrakeTime  1.5}
			{StopTime   3}
			{Gears   0.2 0.4 1}
			{RearGears  0.25}
		}
		{FuelBag
			{volume 530 }	
			{rate	110 }   
			{remain 530 }
			{fuel "fuel"}
		}
		{speed
        	{Normal  10}
        	{Slow  16}
    	}
	}
{Extension "cromwell_mk_vii.mdl"}
}