{game_entity
	(include "/properties/car.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{props "vehicle" "car" "wheeled" "turn_over" "vision_vehicle_low" "cannon"  "flak"}
	{PatherId "vehicle.small"}
	("armor_light" args 1)
	
	{Volume "Body"
		{tags "body"}
		{component "body"}
		{thickness 8}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")


	("crew_2_human")

	{placer
		{place "driver"		{group "driver"} {visor "driver"}}
		{place "commander"	{group "driver"} {visor "main"} {turnoff {targetable 0}}}
	}

	("board_animation" place(driver) board_begin(board_willis_driver) board_end(seat_Willis_driver) base(doorleft))
	("board_animation" place(commander) board_begin(board_willis) board_end(seat_Willis_passenger) base(doorright))


	{extender "inventory"
		{box
			{item "ammo rocket_v1" 6}
		}
	}
	{Weaponry
		{place "rocket"
			{weapon "weapon rocket_v1" filling "ammo rocket_v1" 1} 
			{barrels 1}
			{shooter "shooter"}
			{charger "driver"}
			{gunner "driver"}
			{basic}
		}
	}
	
	{mass 1250}
	{Chassis
		{Locomotion ("locomotion")
			{Maxspeed 40}
	  		{maxSpeedAtMaxTurn 15}
		}
		{FuelBag
			{volume 60 }	
			{rate	10 }   
			{remain 60 }
			{fuel "fuel"}
		}
		{speed
			{Normal  12}
			{Slow  25}
		}
		("car_trace" fx("tracewheel01a") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel01a") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
	}
	{Extension "Vergeltungswaffe1.mdl"}


}
