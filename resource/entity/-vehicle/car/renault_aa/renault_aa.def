{entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{props "vehicle" "car" "wheeled" "with_doors" "flak" "barrel1" "vision_vehicle_medium" "muzzle_zenite"}

	
  ("crew_2_human")
  ("board_animation"
    place(driver)
    board_begin(board_big-highcar_driver)
    board_end(seat_driver_car)
    base(doorleft)
  )
  ("board_animation"
    place(commander)
    board_begin(board_big-highcar)
    board_end(seat_passenger)
    base(doorright)
  )
	{boarder
		{anm "gunner"
			{forward	{begin	"board_LowArmor"}	{end "seat_driver_car" 10}}
			{reverse 	{end	"emit_tank_1"}}
		}
   	}

	("crew_human" place(gunner)		bone(gunner)	door(emit3) animation(gunner))

         {placer
		{place "driver" {visor "driver"} {group "crew"}}
		{place "gunner" {visor "gunner"} {group "crew"}  {openForCrush}}
		{place "commander" {visor "driver"} {group "crew"}}
	}

	{placer
		{place "driver" 	{group "crew"}}
		{place "gunner" 	{group "crew"}}
		{place "commander"	{group "crew"}}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	{Weaponry
		{place "gun"
                  {RestoreIKAfterAim}
			{weapon "37mm_mle1930" filling "bullet37 zenite" 5}  
			{charger "gunner"}
			("abm_zenite")
		}
	}

	{sensor
		{visor "gunner"
			{vision "vehicle_top_mgun"}
			{bone "visor1"}
		}
	}
	
	{extender "inventory"
		{box
			{item "bullet37 zenite" 900}
						{item "repair_kit" 2}
 
		}
	}

	{mass 4000}
  {Chassis
    {Locomotion ("locomotion")
      {TurnRadius 6}
      {MaxSpeed   36}
      {maxSpeedAtMaxTurn 12}
    }
	{FuelBag
		{volume 82 }	
		{rate	20 }   
		{remain 82 }
		{fuel "fuel"}
	}
	{speed
        {Normal  12}
        {Slow  20}
    }
	("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
  }
	{Extension "renault_aa.mdl"}
}